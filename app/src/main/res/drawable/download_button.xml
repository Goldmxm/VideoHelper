<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false" >
        <shape>
            <!--<solid android:color="#087C67"/>-->
            <!--<stroke android:width="0.5dp" android:color="#C8C8C8"/>-->
            <corners
                android:topRightRadius="50dp"
                android:bottomLeftRadius="50dp"
                android:topLeftRadius="50dp"
                android:bottomRightRadius="50dp" />
            <gradient
                android:startColor="@color/ThemeColor"
                android:centerColor="@color/ThemeColor"
                android:endColor="@color/ThemeColor"
                android:angle="0"/>
        </shape>
    </item>

    <item android:state_pressed="true">
        <shape>
            <!--<solid android:color="#087C67"/>-->
            <!--<stroke android:width="0.5dp" android:color="#C8C8C8"/>-->
            <corners
                android:topRightRadius="50dp"
                android:bottomLeftRadius="50dp"
                android:topLeftRadius="50dp"
                android:bottomRightRadius="50dp" />
            <gradient
                android:startColor="@color/DARK_ThemeColor"
                android:centerColor="@color/DARK_ThemeColor"
                android:endColor="@color/DARK_ThemeColor"
                android:angle="0" />
        </shape>
    </item>

</selector>